@model IEnumerable<MovieListViewModel>

<h1>Film overzicht</h1>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
        </tr>
    </thead>
    <tbody>
        @foreach (MovieListViewModel vm in Model)
        {
            <tr>
                <td>@vm.Id</td>
                <td>@vm.Title</td>
            </tr>
        }
</table>